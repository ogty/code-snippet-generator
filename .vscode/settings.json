{
  "editor.fontFamily": "Courier",
  "editor.fontSize": 13,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "cSpell.words": [
    "abcdefghijklm",
    "pbcopy",
    "argparse",
    "popen",
    "tput",
    "subparsers",
    "elif",
    "abstractclassmethod",
    "metaclass",
    "ogty",
    "fram",
    "templa",
    "ngth",
    "widt",
    "differen",
    "aaabbcddd",
    "utput",
    "pipenv",
    "codecov"
  ],
  "[json][jsonc]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
